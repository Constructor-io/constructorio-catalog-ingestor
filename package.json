{
  "name": "@constructor-io/ingestor",
  "version": "1.0.0",
  "description": "Package to help ingesting catalog data into Constructor.io ☕️",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "npm run build:es && npm run build:cjs",
    "build:es": "tsc --declaration true --declarationMap true --module esnext --outDir \"./dist/es\"",
    "build:cjs": "tsc --declaration true --declarationMap true --module commonjs --outDir \"./dist/commonjs\"",
    "prepare": "npm run build",
    "prebuild": "rm -rf dist",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noemit",
    "dev": "ts-node-esm --experimental-specifier-resolution=node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Constructor-io/@constructor-io/ingestor.git"
  },
  "keywords": [
    "nodejs",
    "api",
    "catalog",
    "partner",
    "partner-integrations",
    "constructor-io",
    "constructorio",
    "constructor"
  ],
  "author": "Constructor.io",
  "bugs": {
    "url": "https://github.com/Constructor-io/@constructor-io/ingestor/issues"
  },
  "homepage": "https://github.com/Constructor-io/@constructor-io/ingestor#readme",
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/papaparse": "^5.3.5",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "fishery": "^2.2.2",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "got": "^11.8.5",
    "papaparse": "^5.3.2",
    "remeda": "^1.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "test",
      "src"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.ts"
    ],
    "setupFiles": [],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "resetMocks": true
  }
}
